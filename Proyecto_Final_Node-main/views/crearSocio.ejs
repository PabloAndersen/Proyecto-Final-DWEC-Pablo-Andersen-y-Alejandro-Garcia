<%- include("Template/cabecera", {tituloweb: "Pagina de inicio"}) %>
    <div class="container">
        <h1>Agregar nuevo socio</h1>
        <br>
        <form action="/socio" class="my-2" method="POST">
            <input type="text" placeholder="Nombre socio" class="form-control my-2" name="nombre">
            <input type="text" placeholder="Apellidos del socio" class="form-control my-2" name="apellidos">
            <input type="text" placeholder="DNI socio" class="form-control my-2" name="dni">
            <input type="text" placeholder="Telefono del socio" class="form-control my-2" name="telefono">
            <input type="text" placeholder="Dirección del socio" class="form-control my-2" name="direccion">
            <input type="text" placeholder="Código postal del socio" class="form-control my-2" name="codigo_postal">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
<%- include("Template/footer") %>